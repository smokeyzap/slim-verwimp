{% extends 'layouts/app.twig' %}

{% block content %}
    {% include 'layouts/flash.twig' %}
    <div class="col-md-12"><h1>{{lang.label.return_warranty_other}} {{lang.label.history}}</h1><hr></div>
    <div class="col-md-12">
        <table class="table" id="return_warranty_other_history" style="cursor:pointer;">
            <thead>
                <tr>
                    <th>{{lang.label.inv_number}}</th>
                    <th>{{lang.label.date}}</th>
                    <th>{{lang.label.client_number_label}}</th>
                    <th>{{lang.label.return_number_label}}</th>
                    <th>{{lang.label.balie_label}}</th>
                </tr>
            </thead>
            <tbody>
            {% for r in returns_other_history %}
                <tr>
                    <td>{{r.receipt_number}}</td>
                    <td>{{r.date}}</td>
                    <td>{{r.customer_number}}</td>
                    <td></td>
                    <td>{{r.balie}}</td>
                </tr>
            {% endfor %}
            </tbody>
        </table>
    </div>
    <div class="col-md-12">
        <hr>
    </div>
    <div class="col-md-12">
        <table class="table" id="return_warranty_history_items">
            <thead>
                <tr>
                    <th>{{lang.label.item_number}}</th>
                    <th>{{lang.label.quantity}}</th>
                    <th>{{lang.label.name_label}}</th>
                    <th>{{lang.label.reason_label}}</th>
                    <th>{{lang.label.type_label}}</th>
                    <th>{{lang.label.part_number_label}}</th>
                </tr>
            </thead>
        </table>
    </div>
{% endblock %}